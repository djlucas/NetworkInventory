<?xml version="1.0" encoding="ISO-8859-1" ?>
<check>
 <software>
  <!--
  <search name="String" literal="Bool" expected_version="String" include="regex" exclude="regex" verspec="regex"/>
       name/expected_version: self explanatory
       literal: comparison on name is == (case sensitive, no leading or trailing characters)
       include/exclude: regex comparison against complete product name (not previous name field)
	   verspec: regex comparison against complete version (not expected_version)
  -->
  <search name="7-Zip" literal="False" expected_version="16.04" include="" exclude="" verspec="" />
  <!-- Adobe Acrobat DC and Acrobat Reader DC can use a fast track or classic update path...use classic for full product, and fast track for Reader -->
  <search name="Adobe Acrobat DC" literal="False" expected_version="15.006.30279" include="" exclude="" verspec=""/>
  <search name="Adobe Acrobat Reader DC" literal="False" expected_version="15.023.20053" include="" exclude="" verspec=""/>
  <search name="Adobe Acrobat XI" literal="False" expected_version="11.0.19" include="" exclude="" verspec="" />
  <search name="Adobe Acrobat" literal="False" expected_version="Deprecated. Please upgrade to Acrobat XI or DC." include="" exclude="" verspec="^([2-9]|10)" />
  <search name="Adobe AIR" literal="False" expected_version="24.0.0.180" include="" exclude="" verspec="" />
  <!-- Occasionally Version strings differ across the different Flash targets, most times not -->
  <search name="Adobe Flash Player" literal="False" expected_version="24.0.0.194" include="" exclude="" verspec="" />
  <!--
  <search name="Adobe Flash Player" literal="False" expected_version="24.0.0.194" include="ActiveX" exclude="" verspec="" />
  <search name="Adobe Flash Player" literal="False" expected_version="24.0.0.194" include="NPAPI" exclude="" verspec="" />
  <search name="Adobe Flash Player" literal="False" expected_version="24.0.0.194" include="PPAPI" exclude="" verspec="" />
  -->
