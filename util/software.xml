  <search name="Combined Community Codec Pack" literal="False" expected_version="Deprecated. Please replace with K-Lite Codec Pack." include="" exclude="" verspec="" />
  <search name="ConnectWise Internet Client" literal="False" expected_version="16.3.2" include="" exclude="" verspec="" />
  <search name="Citrix Receiver" literal="True" expected_version="4.6" include="" exclude="" verspec="" />
