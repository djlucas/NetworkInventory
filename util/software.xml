       ...We don't care! We want only corp version of Chrome so that it can be controlled via GPO -->
  <search name="Google Chrome" literal="False" expected_version="55.0.2883.87" include="" exclude="" verspec="" />
  <search name="Google Earth" literal="False" expected_version="7.1.7.2606" include="" exclude="" verspec="" />
  <search name="GPL Ghostscript" literal="False" expected_version="9.20" include="" exclude="" verspec="" />
